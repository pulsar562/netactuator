#
# This is the configuration of the netactuator to be accessed by Apache.
#

Alias /netactuator /usr/local/netactuator/web
<Directory /usr/local/netactuator/web/>
    #AddDefaultCharset ISO-8859-1
    Options FollowSymLinks
    AllowOverride None
    Order deny,allow
    Allow from all
</Directory>
